<view class="page">
    <view class="page__hd">
        <view class="page__title">文章列表</view>
        <view class="page__desc">文章</view>
    </view>
    <block wx:for-items="{{result}}" wx:key="{{index}}" class="weui-article__section">
        <view style="width:100%; height: 50rpx"></view>

        <view class="weui-article__h1">第{{index}}条数据 </view>
        <view class="weui-article__h2">{{item.desc}}</view>
        <view class="weui-article__p">地址{{item.url}}</view>
    </block>
</view>